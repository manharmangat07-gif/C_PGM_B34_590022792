#include <stdio.h>

int main() {
    int seconds, hours, minutes, secs;

    printf("Enter time in seconds: ");
    scanf("%d", &seconds);

    hours = seconds / 3600;
    seconds = seconds % 3600;

    minutes = seconds / 60;
    secs = seconds % 60;

    printf("Time = %d:%d:%d", hours, minutes, secs);

    return 0;
}
